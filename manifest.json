{
    "name": "TLD Phishing Protection",
    "description": "Helps defend against phishing attacks from .zip/.mov TLDs!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "webNavigation",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "*:/\/*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}